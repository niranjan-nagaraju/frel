CC          =   @CC@
CFLAGS      =   @CFLAGS@
OBJECTS     =   prand_trials.o
DEFINES     +=  @DEFS@
LIBS        =   -L../../lib/ -lnet @ADDITIONAL_LIBS@

.c.o:
	$(CC) $(CFLAGS) $(DEFINES) -c $< -o $@

all:    prand_trials

prand_trials: $(OBJECTS)
	$(CC) prand_trials.o $(LIBS) -o ./prand_trials

clean:
	rm -f *.core prand_trials *.o

distclean: clean
	rm -f Makefile

